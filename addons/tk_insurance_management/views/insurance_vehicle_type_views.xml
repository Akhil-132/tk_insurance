<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- View for vehicle body type -->
    <record id="view_vehicle_type_tree" model="ir.ui.view">
        <field name="name">vehicle.type.tree</field>
        <field name="model">insurance.vehicle.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_vehicle_type_form" model="ir.ui.view">
        <field name="name">vehicle.type.form</field>
        <field name="model">insurance.vehicle.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_vehicle_type" model="ir.actions.act_window">
        <field name="name">vehicle Types</field>
        <field name="res_model">insurance.vehicle.type</field>
        <field name="view_mode">tree,form</field>
    </record>


    <record id="view_cylinder_type_tree" model="ir.ui.view">
        <field name="name">cylinder.type.tree</field>
        <field name="model">cylinder.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_cylinder_type_form" model="ir.ui.view">
        <field name="name">cylinder.type.form</field>
        <field name="model">cylinder.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_cylinder_type" model="ir.actions.act_window">
        <field name="name">Cylinder Types</field>
        <field name="res_model">cylinder.type</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
